<header class='top-page-header'>
 <div>
  <%= link_to image_tag("qeity-logo.png", class:"qeity-icon"), "/" %>
 </div>
  <div class='nav'>
    <ul class='lists'>
      <%#if user_signed_in? %>
      <li><%= link_to 'user neme',"#", class: "user-nickname" %></li>
      <li><%= link_to 'ログアウト', "#", class: "logout" %></li>
      <%#else %>
      <li><%= link_to 'ログイン', "#", class: "login" %></li>
      <li><%= link_to '新規登録', "#", class: "sign-up" %></li>
      <%# end %>
    </ul>
  </div>
  <div class='top-page'>
    <b class='title'>クリエイターオークション Qeity</b>
    <form class="search-form" action="#">
      <input class='input-box' placeholder='何を見つけたいですか？'>
      <button class="search-button">
       <%= image_tag "search3.jpg", class:"search-icon" %>
      </button>
    </form>
  </div>
  <div class='nav-second'>
    <ul class='lists-second'>
      <li><%= link_to 'ジャンル', "#", class:"genre-list" %></li>
      <li><%= link_to 'カテゴリー', "#", class:"category-list" %></li>
    </ul>
  </div>
</header>
